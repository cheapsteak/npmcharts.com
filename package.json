{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "Project template for gulp.js",
  "author": "Riku Rouvila <riku.rouvila@leonidasoy.fi>",
  "license": "MIT",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm-run-all --sequential build:frontend start:server",
    "start:server": "cd server && npm start",
    "dev:frontend": "gulp",
    "dev:server": "cd server && npm run dev",
    "dev": "npm-run-all --parallel dev:*",
    "build:frontend": "gulp build"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "dependencies": {
    "array-to-sentence": "^1.1.0",
    "d3": "^3.5.6",
    "hammerjs": "^2.0.4",
    "is-scoped": "^1.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.10.6",
    "nvd3": "1.8.3",
    "vue": "^1.0.21",
    "vue-router": "^0.7.13",
    "whatwg-fetch": "^0.9.0"
  },
  "devDependencies": {
    "babelify": "^6.1.1",
    "browser-sync": "^2.7.2",
    "browserify": "^10.2.1",
    "connect-modrewrite": "^0.8.2",
    "exorcist": "^0.4.0",
    "gulp": "~3.8.1",
    "gulp-autoprefixer": "1.0.1",
    "gulp-duration": "0.0.0",
    "gulp-jade": "~0.9.0",
    "gulp-replace": "^0.5.3",
    "gulp-rev": "^4.0.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "~1.0.1",
    "gulp-util": "~3.0.1",
    "gulp-watch": "^4.3.4",
    "minimist": "^1.2.0",
    "node-notifier": "^4.2.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.3.4",
    "stringify": "^3.1.0",
    "vinyl-source-stream": "~1.0.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^3.2.1"
  }
}
